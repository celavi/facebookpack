<!-- Page plugin -->
<div class="fb-page" 
	 data-href="{$FbPack.pagePluginUrl}" 
	 data-tabs="{$FbPack.pagePluginTabs}" 
	 data-width="{$FbPack.pagePluginWidth}" 
	 data-height="{$FbPack.pagePluginHeight}" 
	 data-small-header="{$FbPack.pagePluginSmallHeader}" 
	 data-adapt-container-width="{$FbPack.pagePluginAdaptWidth}" 
	 data-hide-cover="{$FbPack.pagePluginHideCover}" 
	 data-show-facepile="{$FbPack.pagePluginShowFaces}">
	<blockquote cite="{$FbPack.pagePluginUrl}" class="fb-xfbml-parse-ignore"><a href="{$FbPack.pagePluginUrl}">{$FbPack.pagePluginPageName}</a></blockquote>
</div>